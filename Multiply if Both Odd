#include <stdio.h>

int main() {
    int a, b;
    float result;

    printf("Enter two numbers: ");
    scanf("%d%d", &a, &b);

    if(a%2!=0 && b%2!=0) {
        result = (float)(a*b);
        printf("Result = %.4f\n", result);
    }
    else
        printf("Both numbers are not odd\n");

    return 0;
}
