#include <stdio.h>

int main() {
    float x, y, z;

    printf("Enter x y z coordinates: ");
    scanf("%f%f%f", &x, &y, &z);

    if(x==0 && y==0 && z==0)
        printf("Origin\n");
    else if(x==0 && y==0)
        printf("Z-axis\n");
    else if(x==0 && z==0)
        printf("Y-axis\n");
    else if(y==0 && z==0)
        printf("X-axis\n");
    else if(x==0)
        printf("YZ-plane\n");
    else
        printf("General position in 3D space\n");

    return 0;
}
