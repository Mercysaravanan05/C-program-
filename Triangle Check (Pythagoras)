#include <stdio.h>
#include <math.h>

int main() {
    float a,b,c;

    printf("Enter base, height and ladder length: ");
    scanf("%f%f%f", &a, &b, &c);

    if(fabs((a*a + b*b) - c*c) < 0.001)
        printf("Forms a Right Triangle\n");
    else
        printf("Does NOT form a Right Triangle\n");

    return 0;
}
