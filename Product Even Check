#include <stdio.h>

int main() {
    int a, b, product;

    printf("Enter two integers: ");
    scanf("%d%d", &a, &b);

    product = a * b;

    if(product % 2 == 0)
        printf("Even Product. Result = %d\n", product / 2);
    else
        printf("Odd Product. Result = %d\n", product / 3);

    return 0;
}
